<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F79AFF"
    tools:context=".start">

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button25"
            android:layout_width="342dp"
            android:layout_height="69dp"
            android:backgroundTint="#E5E5E5"
            android:paddingLeft="200dp"
            android:paddingBottom="6dp"
            android:text="30 min"
            android:textColor="#000000"
            android:textSize="20sp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:padding="15dp"
            app:srcCompat="@drawable/vector3" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="130dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button20"
            android:layout_width="342dp"
            android:layout_height="69dp"
            android:backgroundTint="#E5E5E5"
            android:paddingLeft="200dp"
            android:paddingBottom="6dp"
            android:text="59 kcal"
            android:textColor="#000000"
            android:textSize="20sp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="13dp"
            android:paddingLeft="15dp"
            app:srcCompat="@drawable/vector2" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/map"
            android:layout_width="342dp"
            android:layout_height="69dp"
            android:backgroundTint="#E5E5E5"
            android:paddingLeft="200dp"
            android:paddingBottom="6dp"
            android:text="1.3 km"
            android:textColor="#000000"
            android:textSize="20sp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:paddingLeft="15dp"
            app:srcCompat="@drawable/vector1" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="515dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button14"
            android:layout_width="342dp"
            android:layout_height="69dp"
            android:backgroundTint="#FE6AE6"
            android:paddingLeft="65dp"
            android:paddingBottom="6dp"
            android:text="смарт-устройство"
            android:textColor="#000000"
            android:textSize="20sp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="14dp"
            app:srcCompat="@drawable/vector" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="210dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/steps"
            android:layout_width="342dp"
            android:layout_height="69dp"
            android:backgroundTint="#E5E5E5"
            android:paddingLeft="160dp"
            android:paddingBottom="6dp"
            android:text="1583 steps"
            android:textColor="#000000"
            android:textSize="20sp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:padding="15dp"
            app:srcCompat="@drawable/vector4" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </FrameLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame7"
        android:layout_width="161dp"
        android:layout_height="154dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rectangle_1"
        android:orientation="vertical"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="17dp"
            app:srcCompat="@drawable/vector6" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="137dp"
            android:layout_height="126dp"
            android:layout_marginLeft="12.5dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:paddingTop="30dp"
            app:srcCompat="@drawable/scale" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="24dp"
            android:text="62 kg"
            android:textSize="16sp"
            android:textStyle="bold" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame8"
        android:layout_width="161dp"
        android:layout_height="154dp"
        android:layout_marginStart="220dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rectangle_1"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView88"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="17dp"
            app:srcCompat="@drawable/vector5" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="137dp"
            android:layout_height="126dp"
            android:layout_marginLeft="12.5dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:paddingTop="30dp"
            app:srcCompat="@drawable/one1" />

        <TextView
            android:id="@+id/textView88"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="24dp"
            android:text="82 bmp"
            android:textSize="16sp"
            android:textStyle="bold" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>